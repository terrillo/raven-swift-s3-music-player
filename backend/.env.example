# DigitalOcean Spaces / S3 Configuration (Required)
DO_SPACES_KEY=your_access_key
DO_SPACES_SECRET=your_secret_key
DO_SPACES_BUCKET=your_bucket_name

# Optional S3 Configuration
DO_SPACES_REGION=nyc3                    # Options: nyc3, sfo3, ams3, sgp1, fra1, syd1, blr1
DO_SPACES_ENDPOINT=                      # Custom S3 endpoint URL (leave empty for default)
DO_SPACES_PREFIX=music                   # S3 path prefix for all files

# MusicBrainz Configuration (Optional)
MUSICBRAINZ_CONTACT=your@email.com       # Contact email for better rate limits
MUSICBRAINZ_ENABLED=true                 # Enable MusicBrainz lookups

# Last.fm Configuration (Optional)
LASTFM_API_KEY=your_lastfm_api_key       # Get from https://www.last.fm/api/account/create
LASTFM_ENABLED=true                      # Enable Last.fm fallback for metadata
